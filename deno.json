{
  "name": "animepfp-fusion",
  "version": "1.0.0",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "start": "deno run -A main.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.8.0/",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.5.4",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.3",
    "@preact/signals-core": "https://esm.sh/@preact/signals-core@1.6.0",
    "$std/": "https://deno.land/std@0.216.0/",
    "@google/genai": "https://esm.sh/@google/genai@1.0.1",
    "lucide-preact": "https://esm.sh/lucide-preact@0.460.0?external=preact"
  },
  "lock": false,
  "nodeModulesDir": false
}
